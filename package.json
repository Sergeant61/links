    "bcrypt": "^5.0.1",
    "body-parser": "^1.19.0",
    "bootstrap": "^5.2.0",
    "canvas": "^2.9.1",
    "chai": "^4.3.4",
    "dayjs": "^1.11.0",
    "gulp": "^4.0.2",
    "gulp-sass": "^5.1.0",
    "ip": "^1.1.8",
    "jsdom": "^19.0.0",
    "meteor-node-stubs": "^1.2.1",
    "moment": "^2.29.2",
    "notiflix": "^3.2.2",
    "popper.js": "^1.16.1",
